{
  "name": "ember-web-workers",
  "version": "0.1.1",
  "description": "Service to communicate your application with browser web workers",
  "keywords": [
    "ember-addon",
    "service",
    "workers",
    "web-worker",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/BBVAEngineering/ember-web-workers.git"
  },
  "license": "MIT",
  "author": "BBVAEngineering",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint:hbs": "ember-template-lint .",
    "lint:js": "eslint --max-warnings 0 ./*.js addon app config tests",
    "start": "ember server",
    "test": "COVERAGE=true ember test",
    "test:all": "ember try:each",
    "check-coverage": "istanbul check-coverage coverage/coverage-final.json --statements 95 --functions 100 --branches 80 --lines 95",
    "commit": "git-cz",
    "posttest": "npm run report-coverage && npm run check-coverage && npm run validate-coverage",
    "report-coverage": "istanbul report --include=coverage/coverage-final.json text",
    "validate-coverage": "test \"$(cat coverage/coverage-summary.json | json total.lines.total)\" -gt 0",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "ember-cli-babel": "^7.7.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@ember/optional-features": "^0.7.0",
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^2.1.0",
    "ember-cli": "^3.7.1",
    "ember-cli-code-coverage": "^1.0.0-beta.8",
    "ember-cli-dependency-checker": "^3.0.0",
    "ember-cli-htmlbars": "^3.0.0",
    "ember-cli-htmlbars-inline-precompile": "^2.1.0",
    "ember-cli-inject-live-reload": "^2.0.1",
    "ember-cli-qunit-parameterize": "^1.0.1",
    "ember-cli-template-lint": "^1.0.0-beta.2",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^2.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-qunit": "^4.4.0",
    "ember-resolver": "^5.1.1",
    "ember-sinon": "^4.0.0",
    "ember-source": "~3.11.0",
    "ember-source-channel-url": "^2.0.1",
    "ember-try": "~0.2.23",
    "eslint": "^6.1.0",
    "eslint-config-bbva": "^3.2.2",
    "eslint-plugin-ember": "^6.2.0",
    "eslint-plugin-node": "^9.0.1",
    "husky": "^3.0.0",
    "istanbul": "^0.4.5",
    "json": "^9.0.6",
    "loader.js": "^4.7.0",
    "qunit-dom": "^0.9.0",
    "semantic-release": "^15.13.3"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
